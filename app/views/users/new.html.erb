<h1> Sign Up </h1>
<%= form_for(@user) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br />
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br />
  <!-- <p>Admin? <%= f.check_box :admin, options = {:class => "user_admin"} %></p>
  <br /> -->
  
  <%= f.submit "Create User"%>
<% end %>